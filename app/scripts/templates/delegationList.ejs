<div class="panel panel-default">
  <div class="panel-heading headingOne block-padding" role="tab">
    <div class="panel-title subheader-menu-container"></div>
  </div>


  <div id="checkStatus">
    <div class="panel-body">

      <form class="form-horizontal">

        <% if (showSearch) { %>
          <div class="form-group row">
            <div class="col-sm-2">
              <div class="form-label text-right">User</div>
            </div>
            <div class="col-md-6 userContainer">
              <!-- status Select list here -->
            </div>
            <div class="col-md-2">
              <input type="button" class="btn btn-primary searchBtn" value="Search" />
            </div>
          </div>
          <% } %>

            <div class="form-group buttons-container">
              <div class="text-center available-buttons">
                <input type="button" class="btn btn-primary deleteBtn" value="Delete">
                <input type="button" class="btn btn-primary createBtn" value="New">
              </div>
            </div>

      </form>
    </div>
  </div>
</div>
<div class="table-responsive">
  <table id="delegationTable" class="table table-striped table-bordered table-grey" cellspacing="0" width="100%">
    <thead>
      <tr>
        <th><input type="checkbox" class="checkAll"></th>
        <th>User</th>
        <th>Workflow / Task</th>
        <th>Application Department</th>
        <th>Delegation To / Action</th>
        <th>Time</th>
      </tr>
    </thead>
    <tfoot>
      <tr>
        <th></th>
        <th>User</th>
        <th>Workflow / Task</th>
        <th>Application Department</th>
        <th>Delegation To / Action</th>
        <th>Time</th>
      </tr>
    </tfoot>
    <!-- <tbody id="reqbody"></tbody> -->

  </table>
</div>
