<div class="panel panel-default">
  <div class="panel-title panel-heading subheader-menu-container headingOne">
    <!-- subheader menu here -->
  </div>

  <div class="email-template-container block-padding" style="padding-top:14px; padding-bottom:14px; ">

    <div class="form-group row">
      <div class="col-sm-3">
        <div class="form-label text-right">Process</div>
      </div>
      <div class="col-md-9">
        <%= ProcessDisplayName %>
      </div>
    </div>
    <div class="sep-space"></div>


    <div class="row">
      <div class="col-md-3">
        <div class="form-label text-right">Code</div>
      </div>
      <div class="col-md-9">
        <%= Code %>
      </div>
    </div>
    <div class="sep-space"></div>


    <div class="row">
      <div class="col-md-3">
        <div class="form-label text-right">Worker</div>
      </div>
      <div class="col-md-9">
        <%= Worker %>
      </div>
    </div>
    <div class="sep-space"></div>

    <div class="row">
      <div class="col-md-3">
        <div class="form-label text-right">Rule</div>
      </div>
      <div class="col-md-9 ruleContainer"></div>
    </div>
    <div class="sep-space"></div>

    <div class="row">
      <div class="col-md-3">
        <div class="form-label text-right">Summary</div>
      </div>
      <div class="col-md-9">
        <textarea name="Summary" class="form-control formTextField" rows="8" cols="80"><%= Summary %></textarea>
      </div>
    </div>
    <div class="sep-space"></div>

    <div class="row">
      <div class="col-md-3">
        <div class="form-label text-right">Remark</div>
      </div>
      <div class="col-md-9">
        <textarea name="Remark" class="form-control formTextField" rows="8" cols="80"><%= Remark %></textarea>
      </div>
    </div>
    <div class="sep-space"></div>

    <div class="row">
      <div class="col-md-3">
        <div class="form-label text-right"><span class="red">*</span> Score</div>
      </div>
      <div class="col-md-9">
        <input type="number" class="form-control formTextField" name="Score" value="<%= Score %>">
      </div>
    </div>
    <div class="sep-space"></div>

    <div class="form-group buttons-container">
      <div class="text-center available-buttons">
        <% if (showRules) { %>
          <input type="button" class="btn btn-primary batchDeleteBtn" value="Delete" />
        <% } %>
        <input type="button" class="btn btn-primary saveBtn" value="Save" />
      </div>
    </div>
    <div class="sep-space"></div>


  </div>
</div>
