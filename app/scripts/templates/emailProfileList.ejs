<div class="panel panel-default">
  <div class="panel-heading headingOne block-padding" role="tab">
    <div class="panel-title subheader-menu-container">
      <!-- Single button -->
    </div>
    <!-- <span class="advanced-btn-wrapper">
      <span class="glyphicon glyphicon-menu-down toggle-btn openBtn"></span>
      <span class="glyphicon glyphicon-menu-up toggle-btn closeBtn"></span>

      <span class="advanced-btn">
        <span class="isShow">Show</span>
        <span class="isHide">Hide</span>
        Advanced Search
      </span>
    </span> -->

  </div>


  <div id="checkStatus">
    <div class="panel-body">

      <form class="form-horizontal">
        <% if (showSearch) { %>
          <div class="form-group row">
            <div class="col-md-2">
              <div class="form-label text-right">Profile</div>
            </div>
            <div class="col-md-6 profileUserContainer">
              <!-- status Select list here -->
            </div>
            <div class="col-md-2">
              <% if (showSearch) { %>
                <input type="button" class="btn btn-primary searchBtn" value="Search" id="btnSearchEmailProfile">
              <% } %>
            </div>
          </div>
        <% } %>

        <div class="form-group">
          <label for="status" class="col-md-2 control-label"></label>
          <div class="col-md-6 user-container">

          </div>
        </div>


        <div class="form-group buttons-container">
          <div class="text-center available-buttons">
            <input type="button" class="btn btn-primary createBtn" value="New" id="btnCreateEmailProfile">
          </div>
        </div>

      </form>
    </div>
  </div>
</div>
<div class="table-responsive">
  <table id="profileTable" class="table table-striped table-bordered table-grey" cellspacing="0" width="100%">
    <thead>
      <tr>
        <th>Profile</th>
        <th>Process / Task</th>
        <th>Day of Week</th>
        <th>Time</th>
      </tr>
    </thead>
    <tfoot>
      <tr>
        <th>Profile</th>
        <th>Process / Task</th>
        <th>Day of Week</th>
        <th>Time</th>
      </tr>
    </tfoot>
    <!-- <tbody id="reqbody"></tbody> -->

  </table>
</div>
