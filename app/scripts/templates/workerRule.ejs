<div class="panel panel-default">
  <div class="panel-title panel-heading subheader-menu-container headingOne">
    <!-- subheader menu here -->
  </div>

  <div class="email-template-container block-padding" style="padding-top:14px; padding-bottom:14px; ">

    <div class="form-group row">
      <div class="col-sm-3">
        <div class="form-label text-right">Process</div>
      </div>
      <div class="col-md-9 processContainer">
        <!-- status Select list here -->
      </div>
    </div>

    <div class="row">
      <div class="col-md-3">
        <div class="form-label text-right">Code</div>
      </div>
      <div class="col-md-9">
        <input type="text" class="form-control formTextField" name="Code" value="<%= Code %>">
      </div>
    </div>
    <div class="sep-space"></div>


    <div class="row">
      <div class="col-md-3">
        <div class="form-label text-right">Worker</div>
      </div>
      <div class="col-md-9">
        <textarea name="Worker" class="form-control formTextField" rows="8" cols="80"><%= Worker %></textarea>
      </div>
    </div>
    <div class="sep-space"></div>

    <div class="row">
      <div class="col-md-3">
        <div class="form-label text-right">Worker Type</div>
      </div>
      <div class="col-md-9">
        <input type="text" class="form-control formTextField" name="WorkerType" value="<%= WorkerType %>">
      </div>
    </div>
    <div class="sep-space"></div>

    <div class="row">
      <div class="col-md-3">
        <div class="form-label text-right">Summary</div>
      </div>
      <div class="col-md-9">
        <textarea name="Summary" class="form-control formTextField" rows="8" cols="80"><%= Summary %></textarea>
      </div>
    </div>
    <div class="sep-space"></div>

    <div class="row">
      <div class="col-md-3">
        <div class="form-label text-right">Remark</div>
      </div>
      <div class="col-md-9">
        <textarea name="Remark" class="form-control formTextField" rows="8" cols="80"><%= Remark %></textarea>
      </div>
    </div>
    <div class="sep-space"></div>

    <div class="row">
      <div class="col-md-3">
        <div class="form-label text-right">Score</div>
      </div>
      <div class="col-md-9">
        <input type="number" class="form-control formTextField" name="Score" value="<%= Score %>">
      </div>
    </div>
    <div class="sep-space"></div>

    <div class="form-group buttons-container">
      <div class="text-center available-buttons">
        <% if (showRules) { %>
          <input type="button" class="btn btn-primary batchDeleteBtn" value="Delete" />
        <% } %>
        <input type="button" class="btn btn-primary saveBtn" value="Save" />
      </div>
    </div>
    <div class="sep-space"></div>


    <% if (showRules) { %>
    <div class="title">Rules</div>
    <div id="checkStatus">
      <div class="panel-body">

        <form class="form-horizontal">

          <div class="form-group row">
            <div class="col-sm-2">
              <div class="form-label text-right">User</div>
            </div>
            <div class="col-md-6 userContainer">
              <!-- status Select list here -->
            </div>
          </div>


          <div class="form-group row">
            <div class="col-sm-2">
              <div class="form-label text-right">Worker</div>
            </div>
            <div class="col-md-6 workerContainer">
              <!-- status Select list here -->
            </div>
          </div>
        </form>
        <div class="form-group buttons-container">
          <div class="text-center available-buttons">
            <input type="button" class="btn btn-primary searchBtn" value="Search" />
          </div>
        </div>
        <div class="sep-space"></div>
      </div>
    </div>


    <div>
      <div class="">
        <div class="table-responsive">
          <table id="memberTable" class="table table-striped table-bordered table-grey" cellspacing="0" width="100%">
            <thead>
              <tr>
                <th><input type="checkbox" class="checkAll"></th>
                <th>Last Modified Date</th>
                <th>Modified By</th>
                <th>Summary</th>
                <th>Score</th>
                <th>Start Date</th>
                <th>End Date</th>
              </tr>
            </thead>
            <tfoot>
              <tr>
                <th></th>
                <th>Last Modified Date</th>
                <th>Modified By</th>
                <th>Summary</th>
                <th>Score</th>
                <th>Start Date</th>
                <th>End Date</th>
              </tr>
            </tfoot>
          </table>
        </div>
      </div>
    </div>
    <div class="sep-space"></div>

    <div class="">
      <button class="btn addMemeberBtn">
        <i class="glyphicon glyphicon-plus"></i>
        add
      </button>
      <button class="btn removeMemeberBtn">
        <i class="glyphicon glyphicon-minus"></i>
        remove
      </button>
    </div>
    <div class="sep-space"></div>
    <% } %>
    <!-- </div> -->

  </div>
</div>
