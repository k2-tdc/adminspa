<div class="panel panel-default">
  <div class="panel-title panel-heading subheader-menu-container headingOne">
    <!-- subheader menu here -->
  </div>

  <div class="email-template-container block-padding" style="padding-top:14px; padding-bottom:14px; ">

    <div class="form-group row">
      <div class="col-sm-3">
        <div class="form-label text-right"><span class="red">*</span> Process</div>
      </div>
      <% if (showRules) { %>
        <div class="col-md-9"><span class="form-data"><%= ProcessDisplayName %></span></div>
      <% } else { %>
        <div class="col-md-9 container">
          <div class="processContainer"></div>
          <div class="error-message hidden"></div>
          <!-- status Select list here -->
        </div>
      <% } %>

    </div>

    <div class="row">
      <div class="col-md-3">
        <div class="form-label text-right"><span class="red">*</span> Code</div>
      </div>
      <% if (showRules) { %>
        <div class="col-md-9"><span class="form-data"><%= Code %></span></div>
      <% } else { %>
        <div class="col-md-9 container">
          <div class="">
            <input type="text" class="form-control formTextField" name="Code" field="Code" value="<%= Code %>">
          </div>
          <div class="error-message hidden"></div>
        </div>
      <% } %>

    </div>
    <div class="sep-space"></div>


    <div class="row">
      <div class="col-md-3">
        <div class="form-label text-right"><span class="red">*</span> Worker</div>
      </div>
      <div class="col-md-9 container">
        <div class="">
          <textarea name="Worker" field="Worker" class="form-control formTextField" rows="8" cols="80"><%= Worker %></textarea>
        </div>
        <div class="error-message hidden"></div>
      </div>
    </div>
    <div class="sep-space"></div>

    <div class="row">
      <div class="col-md-3">
        <div class="form-label text-right"><span class="red">*</span> Worker Type</div>
      </div>
      <div class="col-md-9 container">
        <div class="">
          <input type="text" class="form-control formTextField" name="WorkerType" field="WorkerType" value="<%= WorkerType %>">
        </div>
        <div class="error-message hidden"></div>
      </div>
    </div>
    <div class="sep-space"></div>

    <div class="row">
      <div class="col-md-3">
        <div class="form-label text-right">Summary</div>
      </div>
      <div class="col-md-9 container">
        <div class="">
          <textarea name="Summary" field="Summary" class="form-control formTextField" rows="8" cols="80"><%= Summary %></textarea>
        </div>
        <div class="error-message hidden"></div>
      </div>
    </div>
    <div class="sep-space"></div>

    <div class="row">
      <div class="col-md-3">
        <div class="form-label text-right">Remark</div>
      </div>
      <div class="col-md-9 container">
        <div class="">
          <textarea name="Remark" field="Remark" class="form-control formTextField" rows="8" cols="80"><%= Remark %></textarea>
        </div>
        <div class="error-message hidden"></div>
      </div>
    </div>
    <div class="sep-space"></div>

    <div class="row">
      <div class="col-md-3">
        <div class="form-label text-right"><span class="red">*</span> Score</div>
      </div>
      <div class="col-md-9 container">
        <div class="">
          <input type="number" class="form-control formTextField" name="Score" field="Score" value="<%= Score %>">
        </div>
        <div class="error-message hidden"></div>
      </div>
    </div>
    <div class="sep-space"></div>

    <div class="form-group buttons-container">
      <div class="text-center available-buttons">
        <% if (showRules) { %>
          <input type="button" class="btn btn-primary deleteBtn" value="Delete" />
        <% } %>
        <input type="button" class="btn btn-primary saveBtn" value="Save" />
      </div>
    </div>
    <div class="sep-space"></div>


    <% if (showRules) { %>
    <div class="rule-title">Rules</div>
    <div id="checkStatus">
      <div class="panel-body">

        <form class="form-horizontal">

          <div class="form-group row">
            <div class="col-sm-2">
              <div class="form-label text-right">User</div>
            </div>
            <div class="col-md-6 userSelectContainer">
              <!-- status Select list here -->
            </div>
          </div>


          <div class="form-group row">
            <div class="col-sm-2">
              <div class="form-label text-right">Worker</div>
            </div>
            <div class="col-md-6 workerSelectContainer">
              <!-- status Select list here -->
            </div>
          </div>
        </form>
        <div class="form-group buttons-container">
          <div class="text-center available-buttons">
            <input type="button" class="btn btn-primary searchBtn" value="Search" />
          </div>
        </div>
        <div class="sep-space"></div>
      </div>
    </div>


    <div>
      <div class="">
        <div class="table-responsive">
          <table id="memberTable" class="table table-striped table-bordered table-grey" cellspacing="0" width="100%">
            <thead>
              <tr>
                <th><input type="checkbox" class="checkAll"></th>
                <th>Last Modified Date</th>
                <th>Modified By</th>
                <th>Summary</th>
                <th>Score</th>
                <th>Start Date</th>
                <th>End Date</th>
              </tr>
            </thead>
            <tfoot>
              <tr>
                <th></th>
                <th>Last Modified Date</th>
                <th>Modified By</th>
                <th>Summary</th>
                <th>Score</th>
                <th>Start Date</th>
                <th>End Date</th>
              </tr>
            </tfoot>
          </table>
        </div>
      </div>
    </div>
    <div class="sep-space"></div>

    <div class="">
      <button class="btn btn-orange addMemeberBtn">
        <i class="glyphicon glyphicon-plus"></i>
        add
      </button>
      <button class="btn btn-orange removeMemeberBtn">
        <i class="glyphicon glyphicon-minus"></i>
        remove
      </button>
    </div>
    <div class="sep-space"></div>
    <% } %>
    <!-- </div> -->

  </div>
</div>
